{
  "name": "sudweb-talks",
  "version": "2.1.5",
  "description": "Talk visualisation tool used for Call for Papers. Backed by Google Spreadsheets.",
  "private": true,
  "main": "index.html",
  "scripts": {
    "build": "browserify -e app.js -t lessify -o bundle.js",
    "deploy": "gh-pages --repo 'https://github.com/sudweb/talks.git' --src '{bundle,index}.{js,html}' -d .",
    "predeploy": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 0",
    "start": "budo app.js:bundle.js --live --open --port 5000 -- -t lessify"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/sudweb/talks.git"
  },
  "readmeFilename": "README.md",
  "keywords": [
    "sudweb",
    "talks",
    "call",
    "for",
    "paper"
  ],
  "author": "Thomas Parisot (https://oncletom.io)",
  "license": "MIT",
  "dependencies": {
    "angular": "~1.0.8",
    "bootstrap": "^3.3.6",
    "jquery": "^2.2.0",
    "markdown-it": "^5.1.0"
  },
  "devDependencies": {
    "browserify": "^13.0.0",
    "budo": "^8.0.0",
    "gh-pages": "^0.8.0",
    "lessify": "^1.0.1",
    "watchify": "^3.7.0"
  }
}
